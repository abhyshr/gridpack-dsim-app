	-ts_adapt_dt_max 0.08
	-ts_adapt_dt_min 0.01
	-ts_exact_final_time MATCHSTEP
	-ts_monitor
	-snes_monitor
	-pc_type bjacobi
	-sub_pc_type lu
	-sub_pc_factor_mat_ordering_type qmd
